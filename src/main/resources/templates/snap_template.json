    "x": -112903.52727890293,
    "y": 436304.25140252174,
    "width": 416.5714285714283,
    "height": 51.428571428571395,
    "rotation": 0
  },
  {
    "type": "text",
    "content": "<ul><li>...</li></ul>",
    "style": {
      "fillColor": "transparent",
      "fillOpacity": 1,
      "fontFamily": "open_sans",
      "fontSize": 26.00000000000004,
      "textAlign": "left",
      "color": "#1a1a1a"
    },
    "id": "3458764543244631715",
    "parentId": null,
    "origin": "center",
